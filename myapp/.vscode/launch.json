    "version": "0.2.0",
    "configurations": [
        
        {
            "name": "Launch Program",
            "program": "${workspaceFolder}/app.js",
            "request": "launch",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "type": "node"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Development Pascal",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\www",
            "env": {
                "NODE_ENV": "dev_pascal"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Development Phil",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\bin\\www",
            "env": {
                "NODE_ENV": "dev_phil"
            }
        }
    ]
}